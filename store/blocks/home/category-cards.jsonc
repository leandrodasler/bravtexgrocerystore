{
  "flex-layout.row#category-cards": {
    "children": ["slider-layout#category-cards"],
    "props": {
      "blockClass": ["category-cards"],
      "paddingTop": 8,
      "paddingBottom": 6
    },
    "title": "Cards das categorias"
  },

  "slider-layout#category-cards": {
    "props": {
      "itemsPerPage": {
        "desktop": 7,
        "tablet": 4,
        "phone": 2
      },
      "centerMode": {
        "desktop": "disabled",
        "mobile": "to-the-left"
      },
      "infinite": true,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "blockClass": "category-cards"
    },
    "children": [
      "flex-layout.col#category-cards-1",
