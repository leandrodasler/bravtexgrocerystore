  },
  "flex-layout.row#shelf4": {
    "props": {
      "paddingBottom": 6,
      "blockClass": "shelf"
    },
    "children": ["list-context.product-list#demo4"],
    "title": "Home - Prateleira 4"
  },
  "list-context.product-list#demo4": {
