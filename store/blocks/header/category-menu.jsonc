{
	"vtex.menu@2.x:menu#category-menu": {
		"props": {
			"experimentalOptimizeRendering": true,
			"items": [
			{
				"id": "menu-item-category-ofertas",
				"type": "custom",
				"blockClass": "category-menu",
				"iconId": null,
				"highlight": false,
				"itemProps": {
				"type": "internal",
				"href": "/home---ofertas/",
				"noFollow": false,
				"tagTitle": "ofertas",
				"text": "Ofertas"
				}
			},
			{
				"id": "menu-item-category-maisvendidos",
				"type": "custom",
				"blockClass": "category-maisvendidos",
				"iconId": null,
				"highlight": false,
				"itemProps": {
				"type": "internal",
				"href": "/home---maisvendidos/",
				"noFollow": false,
				"tagTitle": "maisvendidos",
				"text": "Mais Vendidos"
				}
			}
			]
		},
		"children": ["flex-layout.row#localization", "menu-item#category"]
		},
	"menu-item#category": {
		"props": {
			"id": "menu-item-category-home",
			"type": "custom",
			"blockClass": "main-category-menu",
			"iconId": null,
			"highlight": false,
			"itemProps": {
				"type": "internal",
				"href": "/apparel---categorias/",
				"noFollow": false,
				"tagTitle": "categorias",
				"text": "Categorias"
			},
			"iconProps": {
				"id": "menu-icon",
				"size": 24,
				"viewBox": "0 0 20 20",
				"activeClassName": "main-menu-category",
				"mutedClassName": "main-menu"
			},
			"iconToTheLeft": true
		},
		"blocks": ["vtex.menu@2.x:submenu#category"]
	},

	"flex-layout.row#localization": {
		"props": {
			"blockClass": "localization-row"
		},
		"children": ["modal-trigger#location"]
	},
	"modal-trigger#location": {
		"children": [
		  	"modal-layout#localization"
		]
	  },
	"current-address-component#localization": {
	},
	  "modal-layout#localization": {
		"children": [
		  "current-address-modal"
		]
	  },
	  "modal-actions.close": {
		  "props": {
			  "label": "Fechar"
		  }
	  }
