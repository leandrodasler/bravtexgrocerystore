{
	"flex-layout.col#top-bar-1": {
		"props": {
			"blockClass": "top-bar-col1"
		},
		"children": ["flex-layout.row#top-bar-row-1"]
	},
	"flex-layout.row#top-bar-row-1": {
		"props": {
			"blockClass": "top-bar-row-1",
			"preventHorizontalStretch": false,
			"horizontalAlign": "left"
		},
		"children": ["rich-text#top-text-1", "menu-item#instagram", "menu-item#facebook", "menu-item#whatsapp"]
	},
	"rich-text#top-text-1": {
		"props": {
			"text": "Como fa√ßo minha primeira compra?",
			"textColor": "c-on-emphasis",
			"blockClass": "text-top-1"
		}
	},
	"menu-item#instagram": {
		"props": {
			"blockClass": "top-instagram",
			"id": "top-instagram",
			"type": "custom",
			"highlight": false,
			"itemProps": {
				"type": "internal",
				"href": "/",
				"noFollow": false,
				"tagTitle": "instagram",
				"text": "Instagram"
			},
			"iconProps": {
				"id": "instagram",
				"size": 16,
				"viewBox": "0 0 16 16"
			},
			"iconToTheLeft": true
		}
	},
	"menu-item#facebook": {
		"props": {
