		"props": {
			"blockClass": "top-whatsapp",
			"id": "top-whatsapp",
			"type": "custom",
			"highlight": false,
			"itemProps": {
				"type": "internal",
				"href": "/",
				"noFollow": false,
				"tagTitle": "whatsapp",
